{
    "name": "phpixie/bundles",
    "description": "Bundle library for PHPixie",
    "keywords": ["http", "psr-7"],
    "homepage": "http://phpixie.com",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Roman Tsiupa",
            "email": "draconyster@gmail.com",
            "homepage": "http://dracony.org"
        }
    ],
    "require": {
        "phpixie/auth-login": "~3.0",
        "phpixie/processors": "~3.0",
        "phpixie/route": "~3.0",
        "phpixie/orm": "~3.0",
        "phpixie/filesystem": "~3.0",
        "phpixie/slice": "~3.0",
        "phpixie/console" : "~3.1"
    },
    "require-dev": {
        "phpixie/test": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "PHPixie\\": "src/PHPixie",
            "PHPixie\\Tests\\": "tests/PHPixie/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
